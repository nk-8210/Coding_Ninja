vector<int> moveZeros(int n, vector<int> a) {
    // Write your code here.
    vector<int>v;
    for(int i=0;i<n;i++)
    {
        if(a[i]!=0)
        {
            v.push_back(a[i]);
        }
    }
    int len=v.size();
    
    int k=n-len;
    for(int i=0;i<k;i++)
    {
        v.push_back(0);
    }
    return v;
}
